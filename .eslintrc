{
  "extends": [
    "standard",
    "plugin:jest/recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": ["jest", "react", "jsx-a11y", "prettier"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "react/jsx-curly-brace-presence": "error"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
